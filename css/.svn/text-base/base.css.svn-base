*,
::before,
::after {
	margin: 0;
	padding: 0;
	-webkit-box-sizing: border-box;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0)
}

a,
article,
aside,
audio,
b,
blockquote,
body,
canvas,
caption,
dd,
details,
div,
dl,
dt,
em,
embed,
fieldset,
figcaption,
figure,
footer,
form,
h1,
h2,
h3,
h4,
h5,
h6,
header,
hgroup,
html,
i,
iframe,
img,
label,
legend,
li,
menu,
nav,
object,
ol,
output,
p,
s,
section,
small,
span,
strong,
sub,
summary,
sup,
table,
tbody,
td,
tfoot,
th,
thead,
time,
tr,
tt,
u,
ul,
var,
video {
	margin: 0;
	padding: 0;
	border: 0;
	cursor: default
}

ol,
ul,
li {
	list-style: none
}

img {
	display: block
}
input{
	border: none;
}
a,
a:link,
a:visited {
	text-decoration: none
}

.clearfix:after {
	content: "";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden
}

html {
	font-size: 100px;
	background-color: #f5f5f5;
}
.line1-overflow{
	overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.layui-layer{
	box-shadow: none !important;
}

/*4S店  2手车顶部轮播及搜索区域公共样式*/
.head {
  width: 100%;
  height: 3.8rem;
}
.head .wrap {
  position: relative;
}
.head .wrap .layui-carousel img {
  width: 100%;
  height: 100%;
}
.head .wrap .search {
  width: 6.9rem;
  height: 0.86rem;
  border-radius: 0.2rem;
  position: absolute;
  top: 0.35rem;
  left: 50%;
  margin-left: -3.45rem;
  opacity: 0.8;
}
.head .wrap .search input {
  width: 100%;
  height: 100%;
  text-align: center;
  border-radius: 0.05rem;
}
.head .wrap .search i {
  font-size: 0.45rem;
  position: absolute;
  top: 0.22rem;
  left: 1.3rem;
}

/*4S店  2手车品牌logo区域公共样式*/
.brand {
  background-color: #fff;
  padding: 0.5rem 0.2rem 0.2rem 0.2rem;
  margin-bottom: 0.2rem;
}
.brand ul {
  display: flex;
  flex-wrap: wrap;
}
.brand ul li {
  width: 25%;
  text-align: center;
  margin-bottom: 0.3rem;
}
.brand ul li img {
  width: 1.26rem;
  height: 0.8rem;
  margin: 0 auto 0.2rem;
}

.active{
	color: #2b76ff;
}
