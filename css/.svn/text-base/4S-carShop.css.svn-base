.head {
  width: 100%;
  height: 1.4rem;
  background: url(../img/图层24@2x.png) no-repeat;
  background-size: 100% 100%;
  text-align: center;
  line-height: 1.4rem;
  font-size: 0.38rem;
  font-weight: bold;
  color: #fff;
  letter-spacing: 0.03rem;
}
section {
  margin-bottom: 0.2rem;
}
section .branch {
  height: 0.9rem;
  background-color: #fff;
  padding: 0.2rem;
  border-bottom: 1px solid #EBEBEB;
}
section .branch a {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
section .branch a .shop-name {
  display: flex;
  align-items: center;
}
section .branch a .shop-name img {
  width: 0.3rem;
  height: 0.3rem;
  margin-right: 0.2rem;
}
section .branch a .shop-name p {
  font-size: 0.3rem;
  font-weight: bold;
  justify-content: flex-start;
}
section .branch a span i {
  font-size: 0.35rem;
}
section ul li {
  background-color: #fff;
  padding: 0.3rem 0.2rem;
  margin-bottom: 0.2rem;
  display: flex;
}
section ul li .car-image {
  width: 2.4rem;
  height: 1.6rem;
  margin-right: 0.4rem;
}
section ul li .car-image img {
  width: 100%;
  height: 100%;
}
section ul li .infor {
  width: 4.3rem;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
section ul li .infor .name {
  font-size: 0.32rem;
  color: #333;
}
section ul li .infor .else-info {
  display: flex;
  justify-content: space-between;
}
section ul li .infor .else-info .left {
  width: 2.08rem;
}
section ul li .infor .else-info .left .price {
  font-size: 0.3rem;
  color: red;
  line-height: 0.5rem;
}
section ul li .infor .else-info .left .introduction {
  font-size: 0.24rem;
  color: #999;
  line-height: 0.5rem;
}
section ul li .infor .else-info .test-drive {
  align-self: flex-end;
}
section ul li .infor .else-info .test-drive button {
  border: 1px solid #2b76ff;
  background-color: #fff;
  color: #2b76ff;
  display: inline-block;
  width: 1.66rem;
  height: 0.64rem;
  line-height: 0.64rem;
  white-space: nowrap;
  text-align: center;
  font-size: 0.28rem;
  border-radius: 2px;
}
body .yourclass {
  width: 100%;
  height: 8.45rem;
  background: url(../img/多边形1@2x.png) no-repeat;
  background-size: 100% 100%;
}
body .yourclass .layui-layer-setwin .layui-layer-close2 {
  right: 0.4rem;
  top: 0.6rem;
}
body .yourclass .layui-layer-ico {
  background-size: 100% 100%;
}
.layer-yuyue {
  display: none;
  position: relative;
}
.layer-yuyue .layui-form {
  width: 5.6rem;
  text-align: center;
  margin: 0 auto;
  padding-top: 1.85rem;
}
.layer-yuyue .layui-form p {
  font-size: 0.34rem;
  color: #2b76ff;
  margin-bottom: 0.7rem;
}
.layer-yuyue .layui-form .layui-form-item {
  border: 1px solid #e6e6e6;
  border-radius: 2px;
  margin-bottom: 0.3rem;
}
.layer-yuyue .layui-form .layui-form-item label {
  width: 100px;
  text-align: left;
  padding: 0.25rem 0.3rem;
}
.layer-yuyue .layui-form .layui-form-item .layui-input-block {
  margin-left: 100px;
}
.layer-yuyue .layui-form .layui-form-item .layui-input-block .layui-input {
  height: 0.9rem;
  border: none;
  padding-left: 0;
}
.layer-yuyue .layui-form .submit {
  height: 1rem;
  border: none;
}
.layer-yuyue .layui-form .submit .layui-btn {
  width: 100%;
  height: 100%;
  line-height: 1rem;
  background-color: #2b76ff;
  font-size: 0.35rem;
}
.layer-yuyue .close {
  position: absolute;
  right: 0.65rem;
  top: 0.85rem;
}
.layer-yuyue .close i {
  font-size: 0.65rem;
  color: #2b76ff;
}
