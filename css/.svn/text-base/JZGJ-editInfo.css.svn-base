.take-pic {
	width: 100%;
	height: 2.8rem;
	text-align: center;
	padding-top: 1.6rem;
	font-size: 0.32rem;
	font-weight: bold;
	color: #2b76ff;
	background: url("../img/addlicense .png") no-repeat;
	background-size: 100% 100%;
}

.upload-img {
	width: 100%;
	height: 2.8rem;
	margin-bottom: 0.2rem;
	display: none;
}

.upload-img div {
	width: 100%;
	height: 100%;
	background-color: #fff;
	display: flex;
	justify-content: center;
	align-items: center;
}

.upload-img img {
	max-width: 100%;
	max-height: 100%;
}
.layui-form-item {
  height: 1.1rem;
  background: #fff;
  padding: 0 0.35rem;
  margin-bottom: 0.2rem;
  font-size: 0.32rem;
  color: #333;
}
.layui-form-item label {
  width: auto;
  height: 100%;
  padding: 0;
  line-height: 1.1rem;
  font-weight: bold;
}
.layui-form-item label .star {
  display: inline-block;
  width: 0.3rem;
  text-align: center;
  height: 100%;
  color: red;
}
.layui-form-item .layui-input-block {
  height: 100%;
  margin-left: 2rem;
}
.layui-form-item .layui-input-block .layui-input {
  height: 100%;
  border: none;
}
.layui-form-item .layui-input-block .layui-form-select {
  height: 100%;
}
.layui-form-item .layui-input-block .layui-form-select .layui-select-title {
  height: 100%;
}
.layui-form-item .lz-date {
  margin-left: 2.8rem;
  display: flex;
  align-items: center;
}
.layui-form-item .lz-date img {
  width: 0.3rem;
  height: 0.3rem;
}
.layui-form-item .lz-date input {
  width: 100%;
}
.layui-form-item .period-date {
  margin-left: 2.8rem;
  display: flex;
  align-items: center;
}
.layui-form-item .period-date img {
  width: 0.3rem;
  height: 0.3rem;
}
.layui-form-item .period-date input {
  width: 100%;
}
.bottom {
  width: 100%;
  height: 1.1rem;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
}
.bottom button {
  width: 50%;
  height: 100%;
  line-height: 1.1rem;
  padding: 0;
  margin-left: 0 !important;
  border: none;
  border-radius: 0;
  font-size: 0.32rem;
  font-weight: bold;
}
.bottom a {
	width: 50%;
  height: 100%;
  line-height: 1.1rem;
  text-align: center;
  font-size: 0.32rem;
  font-weight: bold;
  background-color: #dfdfdf;
  color: #666;
}
.bottom button{
  background-color: #2b76ff;
  color: #fff;
}
body .yourclass {
  width: 6.7rem;
  height: 5.6rem;
}
.layer-lincese {
  padding-top: 0.5rem;
  height: 5.6rem;
  text-align: center;
  position: relative;
  display: none;
}
.layer-lincese .lincese-img {
  width: 4.9rem;
  height: 3.22rem;
  margin: 0 auto;
  background-color: #eeeeee;
  display: flex;
  justify-content: center;
  align-items: center;
}
.layer-lincese .lincese-img img {
  width: 3.84rem;
  height: 2.68rem;
}
.layer-lincese p {
  font-size: 0.24rem;
  color: #666;
  margin-top: 0.2rem;
  text-align: center;
}
.layer-lincese button {
  width: 100%;
  height: 1rem;
  border: none;
  background-color: #2b76ff;
  color: #fff;
  letter-spacing: 0.05rem;
  font-weight: bold;
  font-size: 0.32rem;
  line-height: 1rem;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
}

.addCar .layui-form-item{
	padding: 0 0 0 0.35rem;
}
.addCar .layui-input-block{
	margin-left: 1.7rem;
}
.addCar .layui-form-radio{
	margin: 0;
	padding: 0;
	line-height: 1.1rem;
	margin-right: 0.2rem;
}
.addCar .layui-form-radio i{
	margin-right: 0.05rem;
	font-size: 0.44rem;
}
.layui-form-radio span{
	font-size: 0.28rem;
}
.choosePlate{
	display: flex;
}
.choosePlate div{
	width: 1rem;
	display: flex;
	align-items: center;
}
.choosePlate div input{
	width: 100%;
	text-align: center;
	padding-left: 0;
}
.choosePlate div i{
	font-size: 0.3rem;
}

/*键盘输入拍照*/
.cbp-spmenu {
	background-color: #fff;
	position: fixed;
}

.cbp-spmenu-vertical {
	width: 240px;
	height: 100%;
	top: 0;
	z-index: 1000;
}



.cbp-spmenu-horizontal {
	width: 100%;
	left: 0;
	z-index: 1000;
	overflow: hidden;
}


.cbp-spmenu-bottom {
	bottom: -160px;
}

.cbp-spmenu-bottom.cbp-spmenu-open {
	bottom: 0px;
}

.cbp-spmenu{
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	transition: all 0.3s ease;
}
.ul_pro{
	background-color: #CED3D9;
    text-align: center;
    padding: 4px 2px;
    font-size: 14px;
    overflow: hidden;
}
.ul_pro li{
	float: left;
    width: 11.11%;
    padding: 2px;
    box-sizing: border-box;
}
.ul_pro li span{
	display: block;
    background-color: #fff;
    border-radius: 4px;
    box-shadow: 2px 2px 2px #888888;
    max-width: 48px;
    margin: 0 auto;
    line-height: 32px;
    padding-top: 2px;
}

.mask{
	width: 100%;
	height: 100%;
	background-color: #000;
	opacity: 0.7;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 800;
	display: none;
}

