.layui-form-item{
	background-color: #fff;
	padding: 0 0.3rem;
	margin-bottom: 0.2rem;
	font-size: 0.32rem;
	height: 1rem;
	line-height: 1rem;
	display: flex;
	justify-content: space-between;
}
.layui-form-label{
	width: auto;
	line-height: 1rem;
	padding: 0;
}
.layui-form-item:after{
	content: none;
}
.cash{
	color: #2b76ff;
}
.descripe{
	padding-left: 0.3rem;
	color: #666;
	font-size: 0.24rem;
	line-height: 0.8rem;
}
.layui-form-radio{
	margin: 0;
}
.layui-form-radio i{
	margin-right: 0;
}
.wexin-pay label{
	display: flex;
	align-items: center;
}
.wexin-pay img{
	width: 0.7rem;
	height: 0.7rem;
	margin-right: 0.2rem;
}
.layui-btn{
	width: 100%;
	height: 1.1rem;
	line-height: 1.1rem;
	background-color: #2b76ff;
	color: #fff;
	font-size: 0.36rem;
	position: fixed;
	left: 0;
	bottom: 0;
}
